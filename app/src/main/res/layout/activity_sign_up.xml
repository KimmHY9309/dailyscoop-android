<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.user.SignUpActivity">

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_ios_24" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:fontFamily="@font/spoqa_han_sans_neo_medium"
        android:text="@string/signup"
        android:textSize="21sp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/spoqa_han_sans_neo_bold"
        android:text="@string/app_name"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <ImageView
        android:id="@+id/tap1"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:srcCompat="@drawable/profile_image" />

    <TextView
        android:id="@+id/taptext1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="@string/email"
        android:textColor="@color/disableGray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/tap1"
        app:layout_constraintStart_toStartOf="@+id/tap1"
        app:layout_constraintTop_toTopOf="@+id/tap1" />

    <ImageView
        android:id="@+id/tap11"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tap1"
        app:srcCompat="@drawable/profile_image" />

    <TextView
        android:id="@+id/taptext11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="@string/certification"
        android:textColor="@color/disableGray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/tap11"
        app:layout_constraintStart_toStartOf="@+id/tap11"
        app:layout_constraintTop_toTopOf="@+id/tap11" />

    <ImageView
        android:id="@+id/tap2"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tap11"
        app:srcCompat="@drawable/profile_image" />

    <TextView
        android:id="@+id/taptext2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="@string/nickname"
        android:textColor="@color/disableGray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/tap2"
        app:layout_constraintStart_toStartOf="@+id/tap2"
        app:layout_constraintTop_toTopOf="@+id/tap2" />

    <ImageView
        android:id="@+id/tap3"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tap2"
        app:srcCompat="@drawable/profile_image" />

    <TextView
        android:id="@+id/taptext3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="@string/password"
        android:textColor="@color/disableGray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/tap3"
        app:layout_constraintStart_toStartOf="@+id/tap3"
        app:layout_constraintTop_toTopOf="@+id/tap3" />

    <ImageView
        android:id="@+id/tap4"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tap3"
        app:srcCompat="@drawable/profile_image" />

    <TextView
        android:id="@+id/taptext4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/spoqa_han_sans_neo_regular"
        android:text="@string/passwordC"
        android:textColor="@color/disableGray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/tap4"
        app:layout_constraintStart_toStartOf="@+id/tap4"
        app:layout_constraintTop_toTopOf="@+id/tap4" />

    <EditText
        android:id="@+id/editTextTextEmailAddress4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="80dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/tap1"
        app:layout_constraintEnd_toEndOf="@+id/tap1"
        app:layout_constraintStart_toEndOf="@+id/taptext4"
        app:layout_constraintTop_toTopOf="@+id/tap1"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextEmailAddress5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="80dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/tap2"
        app:layout_constraintEnd_toEndOf="@+id/tap2"
        app:layout_constraintStart_toEndOf="@+id/taptext4"
        app:layout_constraintTop_toTopOf="@+id/tap2"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="80dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/tap3"
        app:layout_constraintEnd_toEndOf="@+id/tap3"
        app:layout_constraintStart_toEndOf="@+id/taptext4"
        app:layout_constraintTop_toTopOf="@+id/tap3"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editTextTextPassword4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="80dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="@+id/tap4"
        app:layout_constraintEnd_toEndOf="@+id/tap4"
        app:layout_constraintStart_toEndOf="@+id/taptext4"
        app:layout_constraintTop_toTopOf="@+id/tap4"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="80dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/tap11"
        app:layout_constraintEnd_toEndOf="@+id/tap11"
        app:layout_constraintStart_toEndOf="@+id/taptext4"
        app:layout_constraintTop_toTopOf="@+id/tap11"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/button"
        style="@android:style/Widget.Material.Light.Button.Inset"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/yellow_rectangle"
        android:fontFamily="@font/spoqa_han_sans_neo_medium"
        android:text="@string/cert"
        android:textColor="@color/white"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/tap1"
        app:layout_constraintEnd_toEndOf="@+id/tap1"
        app:layout_constraintStart_toEndOf="@+id/editTextTextEmailAddress4"
        app:layout_constraintTop_toTopOf="@+id/tap1"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button2"
        style="@android:style/Widget.Material.Light.Button.Inset"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/yellow_rectangle"
        android:fontFamily="@font/spoqa_han_sans_neo_medium"
        android:text="@string/ok"
        android:textColor="@color/white"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/tap11"
        app:layout_constraintEnd_toEndOf="@+id/tap11"
        app:layout_constraintStart_toEndOf="@+id/editTextTextEmailAddress4"
        app:layout_constraintTop_toTopOf="@+id/tap11"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/button3"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/yellow_rectangle"
        android:fontFamily="@font/spoqa_han_sans_neo_medium"
        android:text="@string/signup"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tap4"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="@+id/taptext4"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        app:srcCompat="@drawable/ic_baseline_check_24" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/spoqa_han_sans_neo_light"
        android:text="@string/signup_val1"
        android:textColor="@color/palpal"
        app:layout_constraintEnd_toEndOf="@+id/button3"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/taptext4"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        app:srcCompat="@drawable/ic_baseline_check_24" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/spoqa_han_sans_neo_light"
        android:text="@string/signup_val2"
        android:textColor="@color/palpal"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/button3"
        app:layout_constraintStart_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/taptext4"
        app:layout_constraintTop_toBottomOf="@+id/textView23"
        app:srcCompat="@drawable/ic_baseline_check_24" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/spoqa_han_sans_neo_light"
        android:text="@string/signup_val3"
        android:textColor="@color/palpal"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

</androidx.constraintlayout.widget.ConstraintLayout>